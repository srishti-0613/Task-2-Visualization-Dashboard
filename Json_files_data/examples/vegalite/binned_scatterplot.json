{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "data/jsondata.json"
  },
  "mark": "circle",
  "encoding": {
    "x": {
      "bin": {
        "maxbins": 10
      },
      "field": "intensity",
      "type": "quantitative"
    },
    "y": {
      "bin": {
        "maxbins": 10
      },
      "field": "likelihood",
      "type": "quantitative"
    },
    "size": {
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}
