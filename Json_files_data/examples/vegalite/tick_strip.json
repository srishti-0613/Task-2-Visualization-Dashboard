{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "Shows the relationship between start_year and likelihood using tick marks.",
  "data": {
    "url": "data/json.json"
  },
  "mark": "tick",
  "encoding": {
    "x": {
      "field": "start_year",
      "type": "quantitative"
    },
    "y": {
      "field": "likelihood",
      "type": "ordinal"
    }
  }
}
